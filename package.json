{
  "name": "DeepFocus-Timer",
  "displayName": "DeepFocus-Timer",
  "description": "A Focus Timer extension for Visual Studio Code that helps you stay productive using Pomodoro and Deep Work sessions. It features customizable focus durations, a pause/play function, automatic break reminders, and a session history tracker to monitor your progress.",
  "publisher": "Deep-Focus-Timer",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.97.0"
  },
  "icon": "./images/DeepFocus-Timer.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/technologygeeks/Focus-TImer-Extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Focus.Timer",
        "title": "DeepFocus-Timer: Start"
      },
      {
        "command": "Focus.Pause",
        "title": "DeepFocus-Timer: Pause"
      },
      {
        "command": "Focus.Play",
        "title": "DeepFocus-Timer: Play"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
